pipeline {
   agent any

   stages {
      stage ('GIT Checkout'){
        steps {
            git changelog: false, poll: false, url: 'https://github.com/OlenaSakhanda1/DQE-CI-CD/tree/develop'
        }
    }

      stage('build') {
         steps {
            sh 'pip3 install -r requirements.txt'
         }
         }


      stage('runpy') {
        steps{      
          sh 'python3 tests.py'
        }
        }

  }
}
